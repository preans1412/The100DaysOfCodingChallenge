145. Binary Tree Postorder Traversal
Link for the Question - https://lnkd.in/d8cxtXA8



class Solution {
public:
vector<int> v;
    vector<int> postorderTraversal(TreeNode* root)
     {
        postorder(root);
        return v;
    }
    void postorder(TreeNode* root)
    {
    if(root== NULL){return;}
        postorder(root->left);
        postorder(root->right);
        v.push_back(root->val);
    }
    
};



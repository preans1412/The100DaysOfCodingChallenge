1528. Shuffle String



https://leetcode.com/problems/shuffle-string/description/



class Solution {
public:
    string restoreString(string s, vector<int>& indices) 
    {

           string str = s;          
           for(int i=0;i<indices.size();i++)
           {
               int n= indices[i];
               str[n]= s[i];
           }
           return str;   
    }
};
